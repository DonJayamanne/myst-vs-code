{
    "name": "myst-highlight",
    "displayName": "myst-highlight",
    "description": "A syntax highlighter for the MyST Markdown format",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/ExecutableBookProject/myst-highlight.git"
    },
    "license": "LICENSE",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "myst",
                "aliases": [
                    "MyST Markdown"
                ],
                "extensions": [
                    ".myst",
                    ".myst.md"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "myst",
                "scopeName": "text.html.myst",
                "path": "./syntaxes/myst.tmLanguage"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.13.1",
        "plist": "^3.0.1",
        "vscode": "^1.1.36"
    },
    "scripts": {
        "build": "node build",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test ./test"
    }
}
